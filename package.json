{
  "name": "cdk-constructs",
  "private": true,
  "version": "0.1.1",
  "license": "MIT",
  "author": {
    "name": "Justin McCormick",
    "email": "me@justinmccormick.com",
    "url": "https://justinmccormick.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justinm/cdk-constructs.git"
  },
  "main": "lib/index.ts",
  "types": "lib/index.d.ts",
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -p cdkdx build",
    "test": "yarn workspaces foreach -p cdkdx test",
    "lint": "yarn workspaces foreach -p cdkdx lint",
    "upgrade:cdk": "yarn workspaces foreach cdkdx upgrade-cdk",
    "package": "yarn workspaces foreach -p cdkdx package",
    "docgen": "yarn workspaces foreach -p cdkdx docgen",
    "release:npm": "yarn workspaces foreach -p cdkdx release npm",
    "release:pypi": "yarn workspaces foreach -p cdkdx release pypi"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "aws-cdk": "1.110.0",
    "cdkdx": "^1.1.0",
    "eslint-config-cdk": "^1.0.0",
    "eslint-plugin-cdk": "^1.0.2",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "semantic-release": "^17.4.4",
    "semantic-release-monorepo": "^7.0.5"
  }
